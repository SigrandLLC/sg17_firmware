package/dash/Makefile:

удалить DASH_WITH_KDB

webface:
	$(CC) -o $(PKG_BUILD_DIR)/kdb kdb.c md5.c
	$(CC) -o $(PKG_BUILD_DIR)/hdb -luClibc++ hdb/hdb.cpp
	$(CC) -o $(PKG_BUILD_DIR)/htpasswd htpasswd.c
	install	htpasswd kdb hdb /usr/bin
	cp $(PKG_BUILD_DIR)/templates $(IDIR_WEBFACE)
	cp kdb.default.zelax $(IDIR_WEBFACE)/etc/kdb.default # if Zelax
	cp files $(IDIR_WEBFACE)
	OEMing /www /etc logos

webface:
	split to kdb hdb htpasswd
	leave www handling by webface
