diff -Naur socat-1.6.0.0_orig/termioscls.c socat-1.6.0.0/termioscls.c
--- socat-1.6.0.0_orig/termioscls.c	1969-12-31 19:00:00.000000000 -0500
+++ socat-1.6.0.0/termioscls.c	2007-05-21 12:15:39.000000000 -0400
@@ -0,0 +1,494 @@
+/* $Id: telnetcls.c,v 1.18 2005/03/13 12:18:12 gerhard Exp $ */
+/* Copyright Gerhard Rieger 2001-2005 */
+/* Published under the GNU General Public License V.2, see file COPYING */
+/* Jedit Local Buffer Properties :indentSize=3:tabSize=8:collapseFolds=0: */
+
+/* this file contains the source for TCP related functions and options */
+
+#include "xiosysincludes.h"
+#include "xio.h"
+#include "xioopts.h"
+#include "error.h"
+#include "sycls.h"
+
+#include "termioscls.h"
+
+#if WITH_TERMIOS
+
+// This is a local function to either read the termios structure directly from
+//  the socket, or call out to a custom termios function.  The custom function
+//  allows socket types like telnet to provide termios data without supporting real
+//  termios
+static int _getCurrentTermios(xiofile_t *sock, termios_data *new_td) {
+   if (!sock) return -1;
+   if (!sock->stream.termios_ctrl.ctrl_termios && !sock->stream.termios_ctrl.read_termios) return -2;
+
+   termios_data *old_td = &sock->stream.termios_ctrl.termios_data;
+   
+   // First clear the mask so that it shows we have no changes
+   new_td->needsService &= ~T_MASK;
+
+   if (sock->stream.termios_ctrl.get_termios) {
+      // We have a custom term reading function, so use it
+      if (sock->stream.termios_ctrl.get_termios(&sock->stream, new_td) != 0) return -3;
+   } else {
+      // We do not have a custom function, so just use the standard termios stuff
+      if (Tcgetattr(sock->stream.fd, &new_td->term) < 0) {
+	 Error3("tcgetattr(%d, %p): %s",
+	    sock->stream.fd, &new_td->term, strerror(errno));
+	 return -2;
+      }
+      if (!sock->stream.termios_ctrl.ignore_modemstate) {
+	 if (Ioctl(sock->stream.fd, TIOCMGET, &new_td->modemState) < 0) {
+	    Warn3("ioctl(%d, TIOCMGET, %p): %s",
+		   sock->stream.fd, (void *)&old_td->modemState, strerror(errno));
+	 }
+      }
+      
+      termios_getChanges(&sock->stream.termios_ctrl, new_td);
+   }
+   return 0;
+}
+
+// This is a local function to either write the termios structure directly from
+//  the socket, or call out to a custom termios function.  The custom function
+//  allows socket types like telnet to provide termios data without supporting real
+//  termios
+static int _setCurrentTermios(xiofile_t *sock, termios_data *new_td) {
+   termios_data *old_td = &sock->stream.termios_ctrl.termios_data;
+   
+   if (new_td->needsService & T_MASK) {
+      Debug1("Changes found for socket (%04X)", (new_td->needsService & T_MASK));
+      if (sock->stream.termios_ctrl.ctrl_termios) {
+	 if (sock->stream.termios_ctrl.set_termios) {
+	    // We have a custom term reading function, so use it
+	    if (sock->stream.termios_ctrl.set_termios(&sock->stream, new_td) != 0) return -3;
+	 } else {
+	    // We do not have a custom function, so just use the standard termios stuff
+	    termios_setChanges(&sock->stream.termios_ctrl, new_td);
+	    if (Tcsetattr(sock->stream.fd, TCSADRAIN, &old_td->term) < 0) {
+	       Warn3("tcsetattr(%d, %p): %s",
+		  sock->stream.fd, &old_td->term, strerror(errno));
+	    }
+	    if (!sock->stream.termios_ctrl.ignore_modemstate) {
+	       if (Ioctl(sock->stream.fd, TIOCMSET, &old_td->modemState) < 0) {
+		  Warn3("ioctl(%d, TIOCMSET, %p): %s",
+			 sock->stream.fd, (void *)&old_td->modemState, strerror(errno));
+	       }
+	    }
+	 }
+      }
+   }
+   return 0;
+}
+
+// This function will be called from the main data loop in socat.c in order to
+//  check each socket for changes to the termios structure
+int termios_updatePorts(void *vsock1, void *vsock2) {
+   termios_data new_td1 = { 0 }, new_td2 = { 0 };
+   xiofile_t *sock1 = vsock1, *sock2 = vsock2;
+   int ret;
+   
+   // Read from both sockets before we try to write to either of them
+   //  this allows us to make sure both have a valid saved tty
+   //  If either fails, it means it is not dealing with the termios stuff, so
+   //  there is no point in continuing
+   if ((ret = _getCurrentTermios(sock1, &new_td1)) != 0) return ret;
+   if ((ret = _getCurrentTermios(sock2, &new_td2)) != 0) return ret;
+
+
+   // Check to see if any changes were made to the sockets, and write them back
+   //  to the other socket
+   if ((ret = _setCurrentTermios(sock1, &new_td2)) != 0) return ret;
+   if ((ret = _setCurrentTermios(sock2, &new_td1)) != 0) return ret;
+   
+   // Finally if everthing passed to this point, save the changes back to the
+   //  local socket.  This allows us to not settle on initial state until both
+   //  sockets are ready.
+   termios_setChanges(&sock1->stream.termios_ctrl, &new_td1);
+   termios_setChanges(&sock2->stream.termios_ctrl, &new_td2);
+   return 0;
+}
+
+// This is a local function to mark the individual modem state changes.  This
+//  information may be needed by some socket types.  (Ie. telnet)
+static void _markModemStateChanges(const termios_data *old_td, termios_data *new_td) {
+   if ((new_td->modemState & TIOCM_LE  ) != (old_td->modemState & TIOCM_LE  )) new_td->modemState |= TIOCM_MOD(TIOCM_LE  );
+   if ((new_td->modemState & TIOCM_DTR ) != (old_td->modemState & TIOCM_DTR )) new_td->modemState |= TIOCM_MOD(TIOCM_DTR );
+   if ((new_td->modemState & TIOCM_RTS ) != (old_td->modemState & TIOCM_RTS )) new_td->modemState |= TIOCM_MOD(TIOCM_RTS );
+   if ((new_td->modemState & TIOCM_ST  ) != (old_td->modemState & TIOCM_ST  )) new_td->modemState |= TIOCM_MOD(TIOCM_ST  );
+   if ((new_td->modemState & TIOCM_SR  ) != (old_td->modemState & TIOCM_SR  )) new_td->modemState |= TIOCM_MOD(TIOCM_SR  );
+   if ((new_td->modemState & TIOCM_CTS ) != (old_td->modemState & TIOCM_CTS )) new_td->modemState |= TIOCM_MOD(TIOCM_CTS );
+   if ((new_td->modemState & TIOCM_CAR ) != (old_td->modemState & TIOCM_CAR )) new_td->modemState |= TIOCM_MOD(TIOCM_CAR );
+   if ((new_td->modemState & TIOCM_RNG ) != (old_td->modemState & TIOCM_RNG )) new_td->modemState |= TIOCM_MOD(TIOCM_RNG );
+   if ((new_td->modemState & TIOCM_DSR ) != (old_td->modemState & TIOCM_DSR )) new_td->modemState |= TIOCM_MOD(TIOCM_DSR );
+}
+
+/* Identifies what has changed between the old and new structures */
+void termios_getChanges(const termios_ctrl *old_tc, termios_data *new_td) {
+   termios_data *old_td = &old_tc->termios_data;
+   if (!old_td->termvalid) {
+      // Save the new td now, but dont mark it valid till later
+      old_td->term = new_td->term;
+      
+      // Mark everything as new, but check to see if we are ignoring modem state
+      if (!old_tc->ignore_modemstate) {
+	 new_td->needsService |= T_MASK;
+	 new_td->modemState |= TIOCM_MOD(0xFFFF);
+      } else
+	 new_td->needsService |= T_MASK & ~T_MODEM_STATE;
+      return;
+   }
+#if HAVE_TERMIOS_ISPEED
+   if (cfgetospeed(&new_td->term) != cfgetospeed(&old_td->term) && !(old_td->needsService & T_OSPEED))
+      new_td->needsService |= T_OSPEED;
+   if (cfgetispeed(&new_td->term) != cfgetispeed(&old_td->term) && !(old_td->needsService & T_ISPEED))
+#endif
+      new_td->needsService |= T_ISPEED;
+   if ((new_td->term.c_cflag & CSIZE) != (old_td->term.c_cflag & CSIZE) && !(old_td->needsService & T_DATA_SIZE))
+      new_td->needsService |= T_DATA_SIZE;
+   if ((new_td->term.c_cflag & (PARENB|PARODD)) != (old_td->term.c_cflag & (PARENB|PARODD)) && !(old_td->needsService & T_PARITY))
+      new_td->needsService |= T_PARITY;
+   if ((new_td->term.c_cflag & CSTOPB) != (old_td->term.c_cflag & CSTOPB) && !(old_td->needsService & T_STOP_SIZE))
+      new_td->needsService |= T_STOP_SIZE;
+   if ((new_td->term.c_cflag & (IXON|IXOFF|CRTSCTS)) != (old_td->term.c_cflag & (IXON|IXOFF|CRTSCTS)) && !(old_td->needsService & T_FLOW_CTRL))
+      new_td->needsService |= T_FLOW_CTRL;
+   if (new_td->modemState != old_td->modemState && !(old_td->needsService & T_MODEM_STATE) && !old_tc->ignore_modemstate) {
+      _markModemStateChanges(old_td, new_td);
+      new_td->needsService |= T_MODEM_STATE;
+   }
+}
+
+/* Applies what has changed in the new structure to the old structure */
+void termios_setChanges(termios_ctrl *old_tc, const termios_data *new_td) {
+   termios_data *old_td = &old_tc->termios_data;
+#if HAVE_TERMIOS_ISPEED
+   if (new_td->needsService & T_OSPEED)      cfsetospeed(&old_td->term, cfgetospeed(&new_td->term));
+   if (new_td->needsService & T_ISPEED)      cfsetispeed(&old_td->term, cfgetispeed(&new_td->term));
+#endif
+   if (new_td->needsService & T_DATA_SIZE)   old_td->term.c_cflag = (old_td->term.c_cflag & ~CSIZE) | new_td->term.c_cflag & CSIZE;
+   if (new_td->needsService & T_PARITY)      old_td->term.c_cflag = (old_td->term.c_cflag & ~(PARENB|PARODD)) | new_td->term.c_cflag & (PARENB|PARODD);
+   if (new_td->needsService & T_STOP_SIZE)   old_td->term.c_cflag = (old_td->term.c_cflag & ~CSTOPB) | new_td->term.c_cflag & CSTOPB;
+   if (new_td->needsService & T_FLOW_CTRL)   old_td->term.c_cflag = (old_td->term.c_cflag & ~(IXON|IXOFF|CRTSCTS)) | new_td->term.c_cflag & (IXON|IXOFF|CRTSCTS);
+   if (new_td->needsService & T_MODEM_STATE && !old_tc->ignore_modemstate) old_td->modemState   = new_td->modemState;
+   old_td->termvalid  = true;
+}
+
+/* Retrieves the port speed from termios */
+unsigned long int termios_getPortSpeed(termios_data *td) {
+#if HAVE_TERMIOS_ISPEED
+   speed_t speed = cfgetospeed(&td->term);
+
+   switch (speed) {
+   case B50:     return (50UL);
+   case B75:     return (75UL);
+   case B110:    return (110UL);
+   case B134:    return (134UL);
+   case B150:    return (150UL);
+   case B200:    return (200UL);
+   case B300:    return (300UL);
+   case B600:    return (600UL);
+   case B1200:   return (1200UL);
+   case B1800:   return (1800UL);
+   case B2400:   return (2400UL);
+   case B4800:   return (4800UL);
+   case B9600:   return (9600UL);
+   case B19200:  return (19200UL);
+   case B38400:  return (38400UL);
+   case B57600:  return (57600UL);
+   case B115200: return (115200UL);
+   case B230400: return (230400UL);
+   case B460800: return (460800UL);
+   default:      return (0UL);
+   }
+#endif
+}
+
+/* Sets the port speed in termios */
+unsigned long int termios_setPortSpeed(termios_data *td, unsigned long int speed) {
+#if HAVE_TERMIOS_ISPEED
+   speed_t ospeed;
+
+   switch (speed) {
+   case 0:          return termios_getPortSpeed(td);
+   case (50UL):     ospeed = B50;     break;
+   case (75UL):     ospeed = B75;     break;
+   case (110UL):    ospeed = B110;    break;
+   case (134UL):    ospeed = B134;    break;
+   case (150UL):    ospeed = B150;    break;
+   case (200UL):    ospeed = B200;    break;
+   case (300UL):    ospeed = B300;    break;
+   case (600UL):    ospeed = B600;    break;
+   case (1200UL):   ospeed = B1200;   break;
+   case (1800UL):   ospeed = B1800;   break;
+   case (2400UL):   ospeed = B2400;   break;
+   case (4800UL):   ospeed = B4800;   break;
+   case (9600UL):   ospeed = B9600;   break;
+   case (19200UL):  ospeed = B19200;  break;
+   case (38400UL):  ospeed = B38400;  break;
+   case (57600UL):  ospeed = B57600;  break;
+   case (115200UL): ospeed = B115200; break;
+   case (230400UL): ospeed = B230400; break;
+   case (460800UL): ospeed = B460800; break;
+   default:         return 0;         break;
+   }
+   
+   td->needsService |= T_OSPEED|T_ISPEED;
+   cfsetospeed(&td->term, ospeed);
+   cfsetispeed(&td->term, ospeed);
+#endif
+   return speed;
+}
+
+/* Retrieves the data size from termios */
+unsigned char termios_getPortDataSize(termios_data *td) {
+   tcflag_t dataSize = td->term.c_cflag & CSIZE;
+
+   switch (dataSize) {
+   case CS5: return ((unsigned char) 5);
+   case CS6: return ((unsigned char) 6);
+   case CS7: return ((unsigned char) 7);
+   case CS8: return ((unsigned char) 8);
+   default:  return ((unsigned char) 0);
+   }
+}
+
+/* Sets the data size in termios */
+unsigned char termios_setPortDataSize(termios_data *td, unsigned char dataSize) {
+   if (dataSize == 0) return termios_getPortDataSize(td);
+   if (dataSize < 5 || dataSize > 8) return 0;
+   
+   td->needsService |= T_DATA_SIZE;
+   td->term.c_cflag &= ~CSIZE;
+   switch (dataSize) {
+   case 5: td->term.c_cflag |= CS5; break;
+   case 6: td->term.c_cflag |= CS6; break;
+   case 7: td->term.c_cflag |= CS7; break;
+   case 8: td->term.c_cflag |= CS8; dataSize = 8; break;
+   }
+   return dataSize;
+}
+
+/* Retrieves the parity settings from termios */
+unsigned char termios_getPortParity(termios_data *td) {
+   if ((td->term.c_cflag & PARENB) == 0)
+      return ((unsigned char) 1);
+
+   if ((td->term.c_cflag & PARENB) != 0 &&
+      (td->term.c_cflag & PARODD) != 0)
+      return ((unsigned char) 2);
+
+   return ((unsigned char) 3);
+}
+
+/* Retrieves the parity settings from termios */
+unsigned char termios_setPortParity(termios_data *td, unsigned char parity) {
+   switch (parity) {
+   case 0: return termios_getPortParity(td);
+   case 1: td->term.c_cflag &= ~(PARENB|PARODD); break; // NONE
+   case 2: td->term.c_cflag  = (td->term.c_cflag & ~(PARENB|PARODD)) | PARENB|PARODD; break; // ODD
+   case 3: td->term.c_cflag  = (td->term.c_cflag & ~(PARENB|PARODD)) | PARENB;        break; // EVEN
+   case 4: // MARK not supported
+   case 5: // SPACE not supported
+   default: return 0;
+   }
+
+   td->needsService |= T_PARITY;
+   return parity;
+}
+
+/* Retrieves the stop bits size from termios */
+unsigned char termios_getPortStopSize(termios_data *td) {
+   if ((td->term.c_cflag & CSTOPB) == 0)
+      return ((unsigned char) 1);
+   else
+      return ((unsigned char) 2);
+}
+
+/* Retrieves the stop bits size from termios */
+unsigned char termios_setPortStopSize(termios_data *td, unsigned char size) {
+   switch (size) {
+   case 0: return termios_getPortStopSize(td);
+   case 1: td->term.c_cflag &= ~CSTOPB; break;
+   case 2: td->term.c_cflag |=  CSTOPB; break;
+   case 3: // 1.5 not supported
+   default: return 0;
+   }
+  
+   td->needsService |= T_STOP_SIZE;
+   return size;
+}
+
+/* Retrieves the flow control status, including DTR and RTS status,
+from PortFd */
+unsigned char termios_getPortFlowControl(termios_data *td, unsigned char Which) {
+   /* Check wich kind of information is requested */
+   switch (Which) {
+   /* BREAK State  */
+   case 4:
+      if (td->breakSignaled)
+         return ((unsigned char) 5);
+      else
+         return ((unsigned char) 6);
+      break;
+
+   /* DTR Signal State */
+   case 7:
+      if (td->modemState & TIOCM_DTR)
+         return ((unsigned char) 8);
+      else
+         return ((unsigned char) 9);
+      break;
+   
+      /* RTS Signal State */
+   case 10:
+      if (td->modemState & TIOCM_RTS)
+         return ((unsigned char) 11);
+      else
+         return ((unsigned char) 12);
+      break;
+
+   /* Com Port Flow Control Setting (inbound) */
+   case 13:
+      if (td->term.c_iflag & IXOFF)
+         return ((unsigned char) 15);
+      if (td->term.c_cflag & CRTSCTS)
+         return ((unsigned char) 16);
+      return ((unsigned char) 14);
+      break;
+
+   /* Com Port Flow Control Setting (outbound/both) */
+   case 0:
+   default:
+      if (td->term.c_iflag & IXON)
+         return ((unsigned char) 2);
+      if (td->term.c_cflag & CRTSCTS)
+         return ((unsigned char) 3);
+      return ((unsigned char) 1);
+      break;
+   }
+}
+
+unsigned char termios_setPortFlowControl(termios_data *td, unsigned char arg) {
+   switch (arg) {
+   /* Com Port Flow Control Setting (outbound/both) */
+   case 0:
+   case 4:
+   case 7:
+   case 10:
+   case 13:
+      return termios_getPortFlowControl(td, arg);
+      break;
+
+   /* BREAK State  */
+   case 5: td->breakSignaled = true; break;
+   case 6: td->breakSignaled = false; break;
+
+   /* No Flow Control (outbound/both) */
+   case 1:
+      td->term.c_iflag &= ~IXON;
+      td->term.c_iflag &= ~IXOFF;
+      td->term.c_cflag &= ~CRTSCTS;
+      break;
+
+   /* XON/XOFF Flow Control (outbound/both) */
+   case 2:
+      td->term.c_iflag |= IXON;
+      td->term.c_iflag |= IXOFF;
+      td->term.c_cflag &= ~CRTSCTS;
+      break;
+
+   /* HARDWARE Flow Control (outbound/both) */
+   case 3:
+      td->term.c_iflag &= ~IXON;
+      td->term.c_iflag &= ~IXOFF;
+      td->term.c_cflag |= CRTSCTS;
+      break;
+
+   /* DTR Signal State */
+   case 8: td->modemState |=  TIOCM_DTR; break; // ON
+   case 9: td->modemState &= ~TIOCM_DTR; break; // OFF
+
+   /* RTS Signal State */
+   case 11: td->modemState |=  TIOCM_RTS; break; // ON
+   case 12: td->modemState &= ~TIOCM_RTS; break; // OFF
+   
+   /* INBOUND FLOW CONTROL is ignored */
+   /* No Flow Control (inbound) */
+   case 14:
+   /* XON/XOFF Flow Control (inbound) */
+   case 15:
+   /* HARDWARE Flow Control (inbound) */
+   case 16:
+      Warn1("Inbound flow control ignored.", NULL);
+      return arg;
+      break;
+   
+   default:
+      Warn1("Requested unsupported flow control.", NULL);
+      return 0;
+      break;
+   }
+
+   switch (arg) {
+   case 8:
+   case 9:
+   case 11:
+   case 12:
+      td->needsService |= T_MODEM_STATE;
+      break;
+
+   default:
+      td->needsService |= T_FLOW_CTRL;
+      break;
+   }
+   return arg;
+}
+
+/* Return the status of the modem control lines (DCD, CTS, DSR, RNG) */
+unsigned char termios_getModemState(termios_data *td) {
+   unsigned char MState = (unsigned char) 0;
+
+   if ((td->modemState & TIOCM_CAR) != 0)
+      MState += (unsigned char) 128;
+   if ((td->modemState & TIOCM_RNG) != 0)
+      MState += (unsigned char) 64;
+   if ((td->modemState & TIOCM_DSR) != 0)
+      MState += (unsigned char) 32;
+   if ((td->modemState & TIOCM_CTS) != 0)
+      MState += (unsigned char) 16;
+   if ((td->modemState & TIOCM_MOD(TIOCM_CAR)) != 0)
+      MState += (unsigned char) 8;
+   if ((td->modemState & TIOCM_MOD(TIOCM_RNG)) != 0)
+      MState += (unsigned char) 4;
+   if ((td->modemState & TIOCM_MOD(TIOCM_DSR)) != 0)
+      MState += (unsigned char) 2;
+   if ((td->modemState & TIOCM_MOD(TIOCM_CTS)) != 0)
+      MState += (unsigned char) 1;
+
+   return (MState);
+}
+
+void termios_setModemState(termios_data *td, unsigned char MState) {
+   td->needsService |= T_MODEM_STATE;
+   
+   if (MState & 128)
+      td->modemState |= TIOCM_CAR;
+   if (MState & 64)
+      td->modemState |= TIOCM_RNG;
+   if (MState & 32)
+      td->modemState |= TIOCM_DSR;
+   if (MState & 16)
+      td->modemState |= TIOCM_CTS;
+   if (MState & 8)
+      td->modemState |= TIOCM_MOD(TIOCM_CAR);
+   if (MState & 4)
+      td->modemState |= TIOCM_MOD(TIOCM_RNG);
+   if (MState & 2)
+      td->modemState |= TIOCM_MOD(TIOCM_DSR);
+   if (MState & 1)
+      td->modemState |= TIOCM_MOD(TIOCM_CTS);
+}
+
+#endif
+
