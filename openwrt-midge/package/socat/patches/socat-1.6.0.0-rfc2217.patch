+
+static void print_control(const char *func, unsigned int c) {
+   switch (c) {
+   case DO:  Debug1("%s(): DO ",       func);     break;
+   case DONT:Debug1("%s(): DONT ",     func);     break;
+   case WILL:Debug1("%s(): WILL ",     func);     break;
+   case WONT:Debug1("%s(): WONT ",     func);     break;
+   case IAC: Debug1("%s(): IAC ",      func);     break;
+   case SE:  Debug1("%s(): SE ",       func);     break;
+   case IAC+SE: Debug1("%s(): IAC|SE ",func);     break;
+   case SB:  Debug1("%s(): SB ",       func);     break;
+   default:  Debug2("%s(): Ctl %02X ", func, c);  break;
+   }
+}
+
+static void print_option(const char *func, unsigned char c) {
+   switch (c) {
+   case NVT_BINARY:         Debug1("%s(): BINARY ",  func);           break;
+   case NVT_ECHO:           Debug1("%s(): ECHO ",    func);           break;
+   case NVT_SUPP_GO_AHEAD:  Debug1("%s(): SUPPRESS GO AHEAD ", func); break;
+   case NVT_COM_PORT_OPTION:Debug1("%s(): COMM PORT OPTION ",  func); break;
+   default:                 Debug2("%s(): Cmd %3d ", func, c);        break;
+   }
+}
+
+static void send_option(struct single *pipe, int type, int opt) {
+   unsigned char buffer[3];
+
+   buffer[0] = IAC;
+   buffer[1] = type;
+   buffer[2] = opt;
+
+   xiowrite_telnet(pipe, buffer, 3);
+}
+
+#if WITH_TERMIOS
+static void send_comport_command(struct single *pipe, int command, int arg) {
+   #define CC_DATA_SZ    94
+   #define CC_FRAME_SZ   6
+   unsigned char buffer[CC_FRAME_SZ + CC_DATA_SZ];
+   unsigned char *obp = buffer, *tmp;
+   int size, i;
+
+   *obp++ = IAC;
+   *obp++ = SB;
+   *obp++ = NVT_COM_PORT_OPTION;
+   *obp++ = (unsigned char) command;
+
+   switch (USR_COM(command)) {
+   case USR_COM_SET_BAUDRATE:
+      SET_VALUE_4(obp, arg);
+      obp += 4;
+      break;
+   case USR_COM_SIGNATURE:
+      // Arg will be 0 when we are generating a request
+      if (arg) {
+	 tmp  = (unsigned char *)arg;
+	 size = strlen((char*)tmp);
+	 size = (size > CC_DATA_SZ?CC_DATA_SZ:size);
+	 for (i = 0; i < size; ++i) {
+	    *obp++ = tmp[i];
+	 }
+      }
+      break;
+   default:
+      SET_VALUE_1(obp, arg);
+      obp += 1;
+      break;
+   }
+   *obp++ = IAC;
+   *obp++ = SE;
+
+   size = (int) (obp - buffer);
+   xiowrite_telnet(pipe, buffer, size);
+}
+
+static void reply_comport_command(struct single *pipe, int command, int arg) {
+   if (IS_CMD_ACTIVE(pipe, command)) {
+      CLR_CMD_ACTIVE(pipe, command);
+      return;
+   }
+
+   send_comport_command(pipe, command, arg);
+}
+
+static void new_comport_command(struct single *pipe, int command, int arg) {
+   SET_CMD_ACTIVE(pipe, command);
+
+   send_comport_command(pipe, command, arg);
+}
+#endif
+
+static void do_option(struct single *pipe, int opt) {
+   if (I_WANT_TO_SUPPORT(pipe, opt)) {
+      SET_I_DO_SUPPORT(pipe, opt);
+      Debug2("%s(): I WANT: %d", __FUNCTION__, opt);
+      if (!I_SENT_IT(pipe, opt)) {
+	 Debug2("%s(): SEND WILL: %d", __FUNCTION__, opt);
+	 SEND_WILL(pipe, opt);
+	 SET_I_SENT_IT(pipe, opt);
+      }
+   }
+   else {
+      Debug2("%s(): SEND WONT: %d", __FUNCTION__, opt);
+      SEND_WONT(pipe, opt);
+   }
+}
+
+static void dont_option(struct single *pipe, int opt) {
+   CLR_I_DO_SUPPORT(pipe, opt);
+}
+
+static void will_option(struct single *pipe, int opt) {
+   if (HE_MAY_SUPPORT(pipe, opt)) {
+      SET_HE_DOES_SUPPORT(pipe, opt);
+      Info2("%s(): HE DOES: %d", __FUNCTION__, opt);
+      if (!HE_RECV_IT(pipe, opt)) {
+         Debug2("%s(): SEND DO: %d", __FUNCTION__, opt);
+         SEND_DO(pipe, opt);
+         SET_HE_RECV_IT(pipe, opt);
+      }
+   }
+   else {
+      Debug2("%s(): SEND DONT: %d", __FUNCTION__, opt);
+      SEND_DONT(pipe, opt);
+   }
+}
+
+static void wont_option(struct single *pipe, int opt) {
+   CLR_HE_DOES_SUPPORT(pipe, opt);
+}
+
+#if WITH_TERMIOS
+static int handle_com_port_command(struct single *pipe, unsigned char *buf, int subsize) {
+   unsigned char cmd = *buf++;
+   int cmdarg;
+   int cmdidx;
+   //int is_notify = 0;
+   //int notify;
+   int consumed = 1; // We already consumed the command
+   #define BUFF_SZ  40
+   char buffer[BUFF_SZ];
+   
+   bool isClient = TELNET_IS_CLIENT(pipe->telnet_ctrl.type);
+   unsigned char replyCmd = (isClient?USR_COM(cmd):RAS_COM(cmd));
+
+   cmdidx = (int) cmd;
+
+   if (cmd >= RAS_COM_START && cmd <= RAS_COM_END) {
+      cmdidx -= RAS_COM_START;
+   }
+
+   //print_command(cmdidx);
+
+   switch (cmd) {
+   case RAS_COM_SIGNATURE:
+   case USR_COM_SIGNATURE:
+      if (subsize == 1) {
+	 Info2("%s(): Receive USR_COM_SIGNATURE request", __FUNCTION__, subsize);
+	 snprintf(buffer, BUFF_SZ, "socat %s", socatversion);
+	 reply_comport_command(pipe, replyCmd, (int)buffer);
+      } else {
+	 snprintf(buffer, subsize, "%s", buf);
+	 Info2("%s(): Receive USR_COM_SIGNATURE (%s)", __FUNCTION__, buffer);
+      }
+      break;
+
+   case RAS_COM_SET_BAUDRATE:
+   case USR_COM_SET_BAUDRATE:
+      cmdarg = GET_VALUE_4(buf);
+      ++consumed;
+      Info3("%s(): Receive %s: %d", __FUNCTION__, (cmd==USR_COM_SET_BAUDRATE?"USR_COM_SET_BAUDRATE":"RAS_COM_SET_BAUDRATE"), cmdarg);
+      cmdarg = termios_setPortSpeed(&pipe->termios_ctrl.termios_data, cmdarg);
+      reply_comport_command(pipe, replyCmd, cmdarg);
+      break;
+
+   case RAS_COM_SET_DATASIZE:
+   case USR_COM_SET_DATASIZE:
+      cmdarg = GET_VALUE_1(buf);
+      ++consumed;
+      Info3("%s(): Receive %s: %d", __FUNCTION__, (cmd==USR_COM_SET_DATASIZE?"USR_COM_SET_DATASIZE":"RAS_COM_SET_DATASIZE"), cmdarg);
+      cmdarg = termios_setPortDataSize(&pipe->termios_ctrl.termios_data, cmdarg);
+      reply_comport_command(pipe, replyCmd, cmdarg);
+      break;
+
+   case RAS_COM_SET_PARITY:
+   case USR_COM_SET_PARITY:
+      cmdarg = GET_VALUE_1(buf);
+      ++consumed;
+      Info3("%s(): Receive %s: %d", __FUNCTION__, (cmd==USR_COM_SET_PARITY?"USR_COM_SET_PARITY":"RAS_COM_SET_PARITY"), cmdarg);
+      cmdarg = termios_setPortParity(&pipe->termios_ctrl.termios_data, cmdarg);
+      reply_comport_command(pipe, replyCmd, cmdarg);
+      break;
+
+   case RAS_COM_SET_STOPSIZE:
+   case USR_COM_SET_STOPSIZE:
+      cmdarg = GET_VALUE_1(buf);
+      ++consumed;
+      Info3("%s(): Receive %s: %d", __FUNCTION__, (cmd==USR_COM_SET_STOPSIZE?"USR_COM_SET_STOPSIZE":"RAS_COM_SET_STOPSIZE"), cmdarg);
+      cmdarg = termios_setPortStopSize(&pipe->termios_ctrl.termios_data, cmdarg);
+      reply_comport_command(pipe, replyCmd, cmdarg);
+      break;
+
+   case RAS_COM_SET_CONTROL:
+   case USR_COM_SET_CONTROL:
+      cmdarg = GET_VALUE_1(buf);
+      ++consumed;
+      Info3("%s(): Receive %s: %d", __FUNCTION__, (cmd==USR_COM_SET_CONTROL?"USR_COM_SET_CONTROL":"RAS_COM_SET_CONTROL"), cmdarg);
+      cmdarg = termios_setPortFlowControl(&pipe->termios_ctrl.termios_data, cmdarg);
+      reply_comport_command(pipe, replyCmd, cmdarg);
+      break;
+
+   case RAS_COM_SET_MODEMSTATE_MASK:
+   case USR_COM_SET_MODEMSTATE_MASK:
+      cmdarg = GET_VALUE_1(buf);
+      ++consumed;
+      Info3("%s(): Receive %s: %d", __FUNCTION__, (cmd==USR_COM_SET_MODEMSTATE_MASK?"USR_COM_SET_MODEMSTATE_MASK":"RAS_COM_SET_MODEMSTATE_MASK"), cmdarg);
+      reply_comport_command(pipe, replyCmd, cmdarg);
+      break;
+
+   case RAS_COM_SET_LINESTATE_MASK:
+   case USR_COM_SET_LINESTATE_MASK:
+      cmdarg = GET_VALUE_1(buf);
+      ++consumed;
+      Info3("%s(): Receive %s: %d", __FUNCTION__, (cmd==USR_COM_SET_LINESTATE_MASK?"USR_COM_SET_LINESTATE_MASK":"RAS_COM_SET_LINESTATE_MASK"), cmdarg);
+      reply_comport_command(pipe, replyCmd, cmdarg);
+      break;
+
+   case RAS_COM_NOTIFY_MODEMSTATE:
+   case USR_COM_NOTIFY_MODEMSTATE:
+      cmdarg = GET_VALUE_1(buf);
+      ++consumed;
+      termios_setModemState(&pipe->termios_ctrl.termios_data, cmdarg);
+      Info3("%s(): Receive %s: %d", __FUNCTION__, (cmd==USR_COM_SET_LINESTATE_MASK?"USR_COM_NOTIFY_MODEMSTATE":"RAS_COM_NOTIFY_MODEMSTATE"), cmdarg);
+      break;
+      
+   case RAS_COM_NOTIFY_LINESTATE:
+   case USR_COM_NOTIFY_LINESTATE:
+      Info1("%s(): Receive _COM_NOTIFY_LINESTATE", __FUNCTION__);
+      break;
+
+   case RAS_COM_FLOWCONTROL_SUSPEND:
+   case USR_COM_FLOWCONTROL_SUSPEND:
+      Info1("%s(): Receive _COM_FLOWCONTROL_SUSPEND", __FUNCTION__);
+      break;
+
+   case RAS_COM_FLOWCONTROL_RESUME:
+   case USR_COM_FLOWCONTROL_RESUME:
+      Info1("%s(): Receive _COM_FLOWCONTROL_RESUME", __FUNCTION__);
+      break;
+
+   case RAS_COM_PURGE_DATA:
+   case USR_COM_PURGE_DATA:
+      Info1("%s(): Receive _COM_PURGE_DATA", __FUNCTION__);
+      break;
+
+   default:
+      Notice2("%s(): Suboption not implimented: %d", __FUNCTION__, cmd);
+      break;
+   }
+   return consumed;
+}
+#endif /* WITH_TERMIOS */
+
+static int handle_suboption(struct single *pipe, unsigned char *suboptp, int subsize) {
+   unsigned char subopt = *suboptp;
+   int i;
+   
+   // Find the end of the sub option
+   for (i = 0; i < subsize -1; ++i) {
+      if (suboptp[i] == IAC && suboptp[i+1] == SE)
+	 break;
+   }
+
+   switch (subopt) {
+#if WITH_TERMIOS
+   case NVT_COM_PORT_OPTION:
+      return handle_com_port_command(pipe, suboptp +1, i -1);
+      break;
+#endif
+   default:
+      Warn2("%s(): Suboption not supported: %d", __FUNCTION__, subopt);
+      break;
+   }
+   return 0;
+}
+
+# define SUBOPT_MAXSIZE    64
+
+#define TELNET_STATE(pipe)  (pipe)->telnet_ctrl.telnet_state
+
+/* This is the private telnet receive function.  This will consume the telnet 
+   control characters, and write back any data */
+ssize_t _xioread_telnet(struct single *pipe, void *buff, size_t bufsize) {
+   unsigned char *data = buff;
+   int i, bytes;
+   
+   unsigned char *buffer[200];
+   for (i = 0, data = (unsigned char *)buffer; i < bufsize; i++) {
+     data += sprintf((char *)data, "%02x", ((unsigned char *)buff)[i]);
+   }
+   *data = '\0';
+   Info2("%s(): Data: %s", __FUNCTION__, buffer);
+   data = buff;
+   
+   for (i = 0, bytes = 0; i < bufsize; i++) {
+      unsigned char c = data[i];
+      switch (TELNET_STATE(pipe)) {
+      case S_DATA:
+	 if (c == IAC) {
+	    TELNET_STATE(pipe) = S_IAC;
+	    print_control(__FUNCTION__, c);
+	 }
+	 else {
+	    data[bytes++] = c;
+	 }
+	 break;
+      case S_IAC:
+	 switch (c) {
+	 case DO:
+	    TELNET_STATE(pipe) = S_DO;
+	    print_control(__FUNCTION__, c);
+	    break;
+	 case DONT:
+	    TELNET_STATE(pipe) = S_DONT;
+	    print_control(__FUNCTION__, c);
+	    break;
+	 case WILL:
+	    TELNET_STATE(pipe) = S_WILL;
+	    print_control(__FUNCTION__, c);
+	    break;
+	 case WONT:
+	    TELNET_STATE(pipe) = S_WONT;
+	    print_control(__FUNCTION__, c);
+	    break;
+	 case SB:
+	    TELNET_STATE(pipe) = S_SB;
+	    print_control(__FUNCTION__, c);
+	    break;
+	 case IAC:
+	 default:
+	    TELNET_STATE(pipe) = S_DATA;
+	    data[bytes++] = c;
+	    break;
+	 }
+	 break;
+      case S_DO:
+	 print_option(__FUNCTION__, c);
+	 TELNET_STATE(pipe) = S_DATA;
+	 do_option(pipe, c);
+	 break;
+      case S_DONT:
+	 print_option(__FUNCTION__, c);
+	 TELNET_STATE(pipe) = S_DATA;
+	 dont_option(pipe, c);
+	 break;
+      case S_WILL:
+	 print_option(__FUNCTION__, c);
+	 TELNET_STATE(pipe) = S_DATA;
+	 will_option(pipe, c);
+	 break;
+      case S_WONT:
+	 print_option(__FUNCTION__, c);
+	 TELNET_STATE(pipe) = S_DATA;
+	 wont_option(pipe, c);
+	 break;
+      case S_SB:
+	 // Handle the sub-option, and update the counter with any consumed characters
+	 i += handle_suboption(pipe, data +i, bufsize -i);
+	 TELNET_STATE(pipe) = S_SE;
+	 break;
+      case S_SE:
+	 if (c == IAC && data[i+1] == SE) {
+	    TELNET_STATE(pipe) = S_DATA;
+	    print_control(__FUNCTION__, c + data[i+1]);
+	    ++i;
+	 }
+	 else {
+	    //TELNET_STATE(pipe) = S_DATA;
+	    //Warn2("%s(): Suboption not terminated: %d", __FUNCTION__, c);
+	 }
+	 break;
+      }
+   }
+   // We counted how many characters that were written back to the buffer
+   //  If everything was consumed, return -1 and set errno to EAGAIN so the loop
+   //  will keep going
+   if (bytes == 0 && bufsize != 0) {
+     errno = EAGAIN;
+     return -1;
+   }
+   return bytes;
+}
