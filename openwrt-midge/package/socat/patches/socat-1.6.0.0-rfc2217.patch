+/*
+ * State control of NVT Com Port Commands
+ */
+# define CMD_INACTIVE				0
+# define CMD_ACTIVE				1
+
+# define SET_CMD_ACTIVE(pipe, cmd)		((pipe)->telnet_ctrl.cmdState[(cmd) % RAS_COM_SIGNATURE] = CMD_ACTIVE)
+# define CLR_CMD_ACTIVE(pipe, cmd)		((pipe)->telnet_ctrl.cmdState[(cmd) % RAS_COM_SIGNATURE] = CMD_INACTIVE)
+
+# define IS_CMD_ACTIVE(pipe, cmd)		((pipe)->telnet_ctrl.cmdState[(cmd) % RAS_COM_SIGNATURE] == CMD_ACTIVE)
+
+# define USR_COM(cmd)				( (cmd) % 100)
+# define RAS_COM(cmd)				(((cmd) % 100) + 100)
