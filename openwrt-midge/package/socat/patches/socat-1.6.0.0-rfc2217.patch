+#if WITH_TERMIOS
+static int xioopen_telnet_get_termios(struct single *pipe, termios_data *new_td) {   
+   if (!pipe->termios_ctrl.termios_data.termvalid && TELNET_IS_CLIENT(pipe->telnet_ctrl.type)) {
+      if (I_DO_SUPPORT(pipe, NVT_COM_PORT_OPTION) || HE_DOES_SUPPORT(pipe, NVT_COM_PORT_OPTION)) {
+	 Info1("Requesting term status from server", NULL);
+	 // I need to ask the remote server for everything I care about
+	 new_comport_command(pipe, USR_COM_SIGNATURE,  0);
+	 new_comport_command(pipe, USR_COM_SET_BAUDRATE, 0);
+	 new_comport_command(pipe, USR_COM_SET_DATASIZE, 0);
+	 new_comport_command(pipe, USR_COM_SET_PARITY,   0);
+	 new_comport_command(pipe, USR_COM_SET_STOPSIZE, 0);
+	 new_comport_command(pipe, USR_COM_SET_CONTROL,  0);
+	 //pipe->termios_ctrl.termios_data.termvalid    = true;
+      } else {
+	 return -1;
+      }
+   } else {
+     new_td->term         = pipe->termios_ctrl.termios_data.term;
+     new_td->modemState   = pipe->termios_ctrl.termios_data.modemState;
+     new_td->needsService = pipe->termios_ctrl.termios_data.needsService;
+   }
+   
+   // Mark all the service bits clear
+   pipe->termios_ctrl.termios_data.needsService &= ~T_MASK;
+   return 0;
+}
+
+static int xioopen_telnet_set_termios(struct single *pipe, termios_data *new_td) {
+   bool isClient = TELNET_IS_CLIENT(pipe->telnet_ctrl.type);
+   
+   if (I_DO_SUPPORT(pipe, NVT_COM_PORT_OPTION) || HE_DOES_SUPPORT(pipe, NVT_COM_PORT_OPTION)) {
+      if (!pipe->termios_ctrl.termios_data.termvalid) {
+	 Info1("Taking settings from other socket", NULL);
+	 // Our saved state has not been initialized yet, so that probably means we just started
+	 //  and we are a server.  We will just blindly use the term from the other socket (it 
+	 //  is most likely a physical tty)
+	 pipe->termios_ctrl.termios_data.termvalid     = true;
+	 pipe->termios_ctrl.termios_data.term          = new_td->term;
+	 pipe->termios_ctrl.termios_data.modemState    = new_td->modemState;
+	 pipe->termios_ctrl.termios_data.needsService &= ~T_MASK;
+      } else {
+	 // If we get here, we have been running for a while and something has changed on
+	 //  the other socket.  If we are running in client mode we will tell the remote server.
+	 //  If we are a server, we will either just save the new state in case the client asks
+	 //  or tell the client about our change.  NOTE: The spec provides server initiated
+	 //  commands but not all clients will support them.  This client will, but it will be turned off by default.
+	 if (isClient || pipe->telnet_ctrl.sendServerCmds) {
+	    if (new_td->needsService & (T_OSPEED|T_ISPEED)) new_comport_command(pipe, (isClient?USR_COM_SET_BAUDRATE:RAS_COM_SET_BAUDRATE), termios_getPortSpeed(new_td));
+	    if (new_td->needsService & T_DATA_SIZE)         new_comport_command(pipe, (isClient?USR_COM_SET_DATASIZE:RAS_COM_SET_DATASIZE), termios_getPortDataSize(new_td));
+	    if (new_td->needsService & T_PARITY)            new_comport_command(pipe, (isClient?USR_COM_SET_PARITY:RAS_COM_SET_PARITY),     termios_getPortParity(new_td));
+	    if (new_td->needsService & T_STOP_SIZE)         new_comport_command(pipe, (isClient?USR_COM_SET_STOPSIZE:RAS_COM_SET_STOPSIZE), termios_getPortStopSize(new_td));
+	    if (new_td->needsService & T_FLOW_CTRL)         new_comport_command(pipe, (isClient?USR_COM_SET_CONTROL:RAS_COM_SET_CONTROL),   termios_getPortFlowControl(new_td, 0));
+	 }
+      }
+      // This should be done for both client and servers, but there is an error client side
+      if (new_td->needsService & T_MODEM_STATE)       new_comport_command(pipe, (isClient?USR_COM_NOTIFY_MODEMSTATE:RAS_COM_NOTIFY_MODEMSTATE),   termios_getModemState(new_td));
+   } else return -1;
+   return 0;
+}
+#endif /* WITH_TERMIOS */
