+/* Some Telnet Special chars */
+
+# define IAC   255
+# define WILL  251
+# define WONT  252
+# define DO    253
+# define DONT  254
+# define SE    240
+# define SB    250
+
+/* Telnet receiver substates */
+
+#define S_DATA  0
+#define S_IAC   1
+#define S_WILL  2
+#define S_WONT  3
+#define S_DO    4
+#define S_DONT  5
+#define S_SB    6
+#define S_SE    7
+
+
+/* Telnet Options stuff */
+
+#define NVT_BINARY             0
+#define NVT_ECHO               1
+#define NVT_SUPP_GO_AHEAD      3
+#define NVT_COM_PORT_OPTION   44
+
+#define NVT_NUMOPTS          256
+
+#define I_WILL   0x01  /* I desire to support it */
+#define I_DO     0x02  /* I do support it */
+#define I_SENT   0x04  /* I desire and already sent it */
+#define HE_WILL  0x10  /* I want he supports it */
+#define HE_DOES  0x20  /* He supports it */
+#define HE_RECV  0x40  /* He recv my response */
+
+#define I_WANT_TO_SUPPORT(pipe, opt)  ((pipe)->telnet_ctrl.nvtOptions[(opt)] & I_WILL)
+#define I_DO_SUPPORT(pipe, opt)       ((pipe)->telnet_ctrl.nvtOptions[(opt)] & I_DO)
+#define I_SENT_IT(pipe, opt)          ((pipe)->telnet_ctrl.nvtOptions[(opt)] & I_SENT)
+
+#define HE_MAY_SUPPORT(pipe, opt)   ((pipe)->telnet_ctrl.nvtOptions[(opt)] & HE_WILL)
+#define HE_DOES_SUPPORT(pipe, opt)  ((pipe)->telnet_ctrl.nvtOptions[(opt)] & HE_DOES)
+#define HE_RECV_IT(pipe, opt)       ((pipe)->telnet_ctrl.nvtOptions[(opt)] & HE_RECV)
+
+#define SET_I_WANT_TO_SUPPORT(pipe, opt)  ((pipe)->telnet_ctrl.nvtOptions[(opt)] |= I_WILL)
+#define SET_I_DO_SUPPORT(pipe, opt)       ((pipe)->telnet_ctrl.nvtOptions[(opt)] |= I_DO)
+#define SET_I_SENT_IT(pipe, opt)          ((pipe)->telnet_ctrl.nvtOptions[(opt)] |= I_SENT)
+
+#define SET_HE_MAY_SUPPORT(pipe, opt)   ((pipe)->telnet_ctrl.nvtOptions[(opt)] |= HE_WILL)
+#define SET_HE_DOES_SUPPORT(pipe, opt)  ((pipe)->telnet_ctrl.nvtOptions[(opt)] |= HE_DOES)
+#define SET_HE_RECV_IT(pipe, opt)       ((pipe)->telnet_ctrl.nvtOptions[(opt)] |= HE_RECV)
+
+#define CLR_I_WANT_TO_SUPPORT(pipe, opt)  ((pipe)->telnet_ctrl.nvtOptions[(opt)] &= ~I_WILL)
+#define CLR_I_DO_SUPPORT(pipe, opt)       ((pipe)->telnet_ctrl.nvtOptions[(opt)] &= ~I_DO)
+#define CLR_I_SENT_IT(pipe, opt)          ((pipe)->telnet_ctrl.nvtOptions[(opt)] &= ~I_SENT)
+
+#define CLR_HE_MAY_SUPPORT(pipe, opt)   ((pipe)->telnet_ctrl.nvtOptions[(opt)] &= ~HE_WILL)
+#define CLR_HE_DOES_SUPPORT(pipe, opt)  ((pipe)->telnet_ctrl.nvtOptions[(opt)] &= ~HE_DOES)
+#define CLR_HE_RECV_IT(pipe, opt)       ((pipe)->telnet_ctrl.nvtOptions[(opt)] &= ~HE_RECV)
+
+
+#define SEND_DO(pipe, opt)     (send_option((pipe), DO,   (opt)))
+#define SEND_DONT(pipe, opt)   (send_option((pipe), DONT, (opt)))
+#define SEND_WILL(pipe, opt)   (send_option((pipe), WILL, (opt)))
+#define SEND_WONT(pipe, opt)   (send_option((pipe), WONT, (opt)))
+
+/* RFC2217 Stuff */
+
+# define GET_VALUE_4(p) ( ((int) (*((p)  )) << 24) + ((int) (*((p)+1)) << 16) \
+			+ ((int) (*((p)+2)) <<  8) + ((int) (*((p)+3))) )
+
+# define GET_VALUE_1(p) (*(p))
+
+# define SET_VALUE_4(p, val) { \
+			*((p))   = (val >> 24) & 0xff; \
+			*((p)+1) = (val >> 16) & 0xff; \
+			*((p)+2) = (val >> 8) & 0xff; \
+			*((p)+3) = val & 0xff; \
+			}
+
+# define SET_VALUE_1(p, val) { \
+			*((p))   = val & 0xff; \
+			}
