+/****** socat global functions ******/
+
+int xioopen_telnet_ipapp_connect(int argc, const char *argv[], struct opt *opts, int xioflags, xiofile_t *fd,
+			 unsigned groups, int socktype,
+			 int ipproto, int protname) {
+   // First lets treat this like a regular raw tcp connection
+   int ret;
+   
+   if (ipproto == IPPROTO_TCP) {
+      ret = xioopen_ipapp_connect(argc, argv, opts, xioflags, fd, groups, socktype, ipproto, protname);
+   } else {
+      ret = xioopen_openssl_connect(argc, argv, opts, xioflags, fd, groups, socktype, ipproto, protname);
+      fd->stream.telnet_ctrl.type |= TELNET_SSL;
+   }
+
+   if (ret != STAT_OK) return ret;
+   
+   // Now override the data type so that the telnet read/write functions will be called
+   fd->stream.dtype = XIODATA_TELNET;
+   
+   // Initialize the termios control structure
+   fd->stream.termios_ctrl.ctrl_termios = true;
+   fd->stream.telnet_ctrl.telnet_state  = S_DATA;
+   fd->stream.telnet_ctrl.type         |= TELNET_CLIENT;
+   
+   
+#if WITH_TERMIOS
+   // Always allow controlling of this stream
+   fd->stream.termios_ctrl.get_termios = xioopen_telnet_get_termios;
+   fd->stream.termios_ctrl.set_termios = xioopen_telnet_set_termios;
+   fd->stream.termios_ctrl.termios_data.needsService = 0;
+#endif
+   
+   
+   // Set telnet options
+   SET_I_WANT_TO_SUPPORT(&fd->stream, NVT_BINARY);
+   SET_HE_MAY_SUPPORT(&fd->stream, NVT_BINARY);
+   
+   
+   SET_HE_MAY_SUPPORT(&fd->stream, NVT_ECHO);
+   
+#if WITH_TERMIOS
+   SET_I_WANT_TO_SUPPORT(&fd->stream, NVT_COM_PORT_OPTION);
+   SET_HE_MAY_SUPPORT(&fd->stream, NVT_COM_PORT_OPTION);
+   //SEND_WILL(&fd->stream, NVT_COM_PORT_OPTION);
+   //SET_I_SENT_IT(&fd->stream, NVT_COM_PORT_OPTION);
+#endif
+
+   SET_I_WANT_TO_SUPPORT(&fd->stream, NVT_SUPP_GO_AHEAD);
+   SET_HE_MAY_SUPPORT(&fd->stream, NVT_SUPP_GO_AHEAD);
+   //SEND_DO(&fd->stream, NVT_SUPP_GO_AHEAD);
+   //SET_HE_RECV_IT(&fd->stream, NVT_SUPP_GO_AHEAD);
+   
+   
+   return ret;
+}
+
+int xioopen_telnet_ipapp_listen(int argc, const char *argv[], struct opt *opts, int xioflags, xiofile_t *fd,
+			 unsigned groups, int socktype,
+			 int ipproto, int protname) {
+   int ret;// = xioopen_ipapp_listen(argc, argv, opts, xioflags, fd, groups, socktype, ipproto, protname);
+   
+   if (ipproto == IPPROTO_TCP) {
+      Info("Creating a raw socket");
+      ret = xioopen_ipapp_listen(argc, argv, opts, xioflags, fd, groups, socktype, ipproto, protname);
+   } else {
+      Info("Creating a ssl socket");
+      ret = xioopen_openssl_listen(argc, argv, opts, xioflags, fd, groups, socktype, ipproto, protname);
+      fd->stream.telnet_ctrl.type |= TELNET_SSL;
+   }
+   if (ret != STAT_OK) return ret;
+   
+   // Now override the data type so that the telnet read/write functions will be called
+   fd->stream.dtype = XIODATA_TELNET;
+   
+   termios_updatePorts(fd, NULL);
+   
+   
+   // Initialize the termios control structure
+   fd->stream.termios_ctrl.ctrl_termios = true;
+   fd->stream.telnet_ctrl.telnet_state  = S_DATA;
+      fd->stream.telnet_ctrl.type      |= TELNET_SERVER;
+   
+   
+#if WITH_TERMIOS
+   // Always allow controlling of this stream
+   fd->stream.termios_ctrl.get_termios = xioopen_telnet_get_termios;
+   fd->stream.termios_ctrl.set_termios = xioopen_telnet_set_termios;
+   fd->stream.termios_ctrl.termios_data.needsService = 0;
+#endif
+   
+   // Set telnet options
+   SET_I_WANT_TO_SUPPORT(&fd->stream, NVT_BINARY);
+   SET_HE_MAY_SUPPORT(&fd->stream, NVT_BINARY);
+   SEND_WILL(&fd->stream, NVT_BINARY);
+   SET_I_SENT_IT(&fd->stream, NVT_BINARY);
+   
+   
+   SET_I_WANT_TO_SUPPORT(&fd->stream, NVT_ECHO);
+   SET_HE_MAY_SUPPORT(&fd->stream, NVT_ECHO);
+   SEND_WILL(&fd->stream, NVT_ECHO);
+   SET_I_SENT_IT(&fd->stream, NVT_ECHO);
+
+   SET_I_WANT_TO_SUPPORT(&fd->stream, NVT_SUPP_GO_AHEAD);
+   SET_HE_MAY_SUPPORT(&fd->stream, NVT_SUPP_GO_AHEAD);
+   SEND_WILL(&fd->stream, NVT_SUPP_GO_AHEAD);
+   SET_I_SENT_IT(&fd->stream, NVT_SUPP_GO_AHEAD);
+   
+#if WITH_TERMIOS
+   SET_I_WANT_TO_SUPPORT(&fd->stream, NVT_COM_PORT_OPTION);
+   SET_HE_MAY_SUPPORT(&fd->stream, NVT_COM_PORT_OPTION);
+   SEND_DO(&fd->stream, NVT_COM_PORT_OPTION);
+   SET_HE_RECV_IT(&fd->stream, NVT_COM_PORT_OPTION);
+#endif
+   
+   return ret;
+}
+ssize_t xioread_telnet(struct single *pipe, void *buff, size_t bufsiz) {
+   int _errno;
+   ssize_t bytes;// = xioread_rawip(pipe, buff, bufsiz);
+   if (TELNET_IS_SSL(pipe->telnet_ctrl.type)) {
+      bytes = xioread_openssl(pipe, buff, bufsiz);
+   } else {
+      do {
+	 bytes = Read(pipe->fd, buff, bufsiz);
+      } while (bytes < 0 && errno == EINTR);
+      if (bytes < 0) {
+	 _errno = errno;
+	 switch (_errno) {
+	 case EPIPE: case ECONNRESET:
+	    Warn4("read(%d, %p, "F_Zu"): %s",
+		  pipe->fd, buff, bufsiz, strerror(_errno));
+	    break;
+	 default:
+	    Error4("read(%d, %p, "F_Zu"): %s",
+		   pipe->fd, buff, bufsiz, strerror(_errno));
+	 }
+	 errno = _errno;
+	 return -1;
+      }
+   }
+   
+   return _xioread_telnet(pipe, buff, bytes);
+   return bytes;
+}
+
+ssize_t xiopending_telnet(struct single *pipe) {
+   if (TELNET_IS_SSL(pipe->telnet_ctrl.type))
+      return xiopending_openssl(pipe);
+   return 0;
+}
+
+ssize_t xiowrite_telnet(struct single *pipe, const void *buff, size_t bytes) {
+   int _errno;
+   ssize_t writt;// = xiowrite_rawip(pipe, buff, bytes);
+   if (TELNET_IS_SSL(pipe->telnet_ctrl.type)) {
+      writt = xiowrite_openssl(pipe, buff, bytes);
+   } else {
+      do {
+	 writt = Write(pipe->fd, buff, bytes);
+      } while (writt < 0 && errno == EINTR);
+      if (writt < 0) {
+	 _errno = errno;
+	 switch (_errno) {
+	 case EPIPE:
+	 case ECONNRESET:
+	    if (pipe->cool_write) {
+	       Notice4("write(%d, %p, "F_Zu"): %s",
+		       pipe->fd, buff, bytes, strerror(_errno));
+	       break;
+	    }
+	    /*PASSTRHOUGH*/
+	 default:
+	    Error4("write(%d, %p, "F_Zu"): %s",
+		   pipe->fd, buff, bytes, strerror(_errno));
+	 }
+	 errno = _errno;
+	 return -1;
+      }
+      if ((size_t)writt < bytes) {
+	 Warn2("write() only wrote "F_Zu" of "F_Zu" bytes",
+	       writt, bytes);
+      }
+   }
+   
+   return writt;
+}
